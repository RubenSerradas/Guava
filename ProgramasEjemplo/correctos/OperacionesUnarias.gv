/**
 * OperacionesUnarias.gv
 * SIN ERRORES.
 *
 * Prueba de operaciones unarias. Se utilizaran variables globales.
 *
 */

declare {
  integer i;
  boolean b;
}

function void main () {

  i := 1;
  b := true;

  /* Incrementos */
  i := i++;
  i := ++i;

  /* Decrementos */
  i := i--;

  /* Combinacion de operadores */

  b := not b;
  b := not not not b;
  b := not (not (not (not b)));

}
