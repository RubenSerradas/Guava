function void main(){
  declare{
    integer i;
  }
  i := 0;
  for (i ; i < 100; i++){
    if ( ((i mod 3) = 0) and ((i mod 5) = 0) ) then{
       print("fizzbuzz");
    }
    else if ( (i mod 3) = 0) then {
      print("fizz");
    } else if ( (i mod 5) = 0 ) then {
      print("buzz");
    }
  } 
}
